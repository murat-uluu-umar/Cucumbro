{
  "name": "Warptimer",
  "description": "This extension will improve your efficiency. It based on time management technique - \"flowtime\".",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "Resources/Icon/clock.png",
    "32": "Resources/Icon/clock1.png",
    "128": "Resources/Icon/clock2.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "nativeMessaging",
    "alarms",
    "notifications",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "Views/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "Scripts/DexieJs/database.js"
      ],
      "run_at": "document_start"
    }
  ]
}