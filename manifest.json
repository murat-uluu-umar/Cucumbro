{
  "name": "Warptimer",
  "description": "This extension will improve your efficiency. It based on time management technique - \"flowtime\".",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "Resources/Icon/clock.png",
    "32": "Resources/Icon/clock1.png",
    "128": "Resources/Icon/clock2.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "nativeMessaging",
    "alarms",
    "notifications",
    "storage"
  ],
  "action": {
    "default_popup": "Views/popup.html"
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self' 'unsafe-hashes';script-src 'self' 'wasm-unsafe-eval'; style-src 'self' 'unsafe-inline'"
 }
}